{
  "name": "MAMA-HD",
  "version": "2.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "icons": {
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
		"webRequest",
		"webRequestBlocking",
		"tabs",
		"notifications",
		"*://*.youku.com/*"
	]
}
